  "source": "pragma solidity ^0.4.21;\n\ncontract Demo {\n    address  public owner;\n\n    constructor () {\n        owner = msg.sender;\n    }\n\n    function changeOwner(address _newOwner) public {\n        require(msg.sender == owner);\n        owner = _newOwner;\n    }\n}",
